-- Table: public.studentinformation

-- DROP TABLE IF EXISTS public.studentinformation;

CREATE TABLE IF NOT EXISTS public.studentinformation
(
    "Stu_Name" character varying(50) COLLATE pg_catalog."default" NOT NULL,
    "Stu_Reg_No" character varying(10) COLLATE pg_catalog."default" NOT NULL,
    "Stu_Phone" character varying(10) COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT studentinformation_pkey PRIMARY KEY ("Stu_Reg_No")
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.studentinformation
    OWNER to postgres;
